{
	"name": "latexrigana",
	"displayName": "latexrigana",
	"description": "LaTeXで漢字に振り仮名を振るためのショートカット。\n \\usepackage{pxrubrica}が必要となります。",
	"version": "0.2.0",
	"publisher": "monndenntomoki",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.latexrigana"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.latexrigana",
				"title": "LaTeXrigana"
			}
		],
		"keybindings": [
			{
				"command": "extension.latexrigana",
				"key": "ctrl+shift+l"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
